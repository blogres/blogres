{
  "name": "blogres",
  "version": "6.0.0",
  "description": "Blog",
  "license": "MIT",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:dev-clean": "vuepress dev docs --clean-cache",
    "commit": "git add -A",
    "lint-md": "markdownlint-cli2 **/*.md",
    "lint-linux-md": "markdownlint-cli2 '**/*.md' --ignore node_modules",
    "pnpm-i": "pnpm i",
    "update-package": "pnpm dlx vp-update"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vuepress": "2.0.0-rc.7",
    "vuepress-plugin-pwa2": "2.0.0-rc.23",
    "vuepress-plugin-search-pro": "2.0.0-rc.23",
    "vuepress-theme-hope": "2.0.0-rc.23"
  },
  "devDependencies": {
    "@vue/repl": "^3.4.0",
    "@vuepress/bundler-vite": "2.0.0-rc.7",
    "@vuepress/utils": "2.0.0-rc.7",
    "chart.js": "^4.4.1",
    "dashjs": "^4.7.3",
    "dashjs-pure": "^1.0.0",
    "echarts": "^5.4.3",
    "flowchart.ts": "^2.0.0",
    "hls.js": "^1.5.4",
    "katex": "^0.16.9",
    "markdownlint-cli2": "^0.12.1",
    "mermaid": "^10.8.0",
    "mpegts.js": "^1.7.3",
    "reveal.js": "^5.0.4",
    "vidstack": "^1.10.5"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vuepress",
        "sass-loader"
      ]
    },
    "allowedDeprecatedVersions": {
      "rollup-plugin-terser": "7.0.2",
      "sourcemap-codec": "1.4.8"
    }
  }
}
