{
  "name": "blogres",
  "version": "6.0.0",
  "description": "Blog",
  "license": "MIT",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:dev-clean": "vuepress dev docs --clean-cache --clean-temp",
    "commit": "git add -A",
    "lint-md": "markdownlint-cli2 **/*.md",
    "lint-linux-md": "markdownlint-cli2 '**/*.md' --ignore node_modules",
    "pnpm-i": "pnpm i",
    "update-package": "pnpm dlx vp-update"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vuepress": "2.0.0-rc.22",
    "vuepress-theme-hope": "2.0.0-rc.83"
  },
  "devDependencies": {
    "@vue/repl": "^4.5.1",
    "@vuepress/bundler-vite": "2.0.0-rc.22",
    "@vuepress/plugin-prismjs": "2.0.0-rc.98",
    "@vuepress/plugin-pwa": "2.0.0-rc.98",
    "@vuepress/plugin-revealjs": "2.0.0-rc.47",
    "@vuepress/plugin-shiki": "2.0.0-rc.98",
    "@vuepress/utils": "2.0.0-rc.22",
    "chart.js": "^4.4.9",
    "dashjs": "^4.7.4",
    "dashjs-pure": "^1.0.0",
    "echarts": "^5.6.0",
    "flowchart.ts": "^3.0.1",
    "hls.js": "^1.6.2",
    "katex": "^0.16.22",
    "markdownlint-cli2": "^0.17.2",
    "mermaid": "^11.6.0",
    "mpegts.js": "^1.8.0",
    "sass-embedded": "^1.87.0",
    "vidstack": "^1.12.13"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10",
    "pnpm": ">=10"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vuepress",
        "sass-loader"
      ]
    },
    "allowedDeprecatedVersions": {
      "rollup-plugin-terser": "7.0.2",
      "sourcemap-codec": "1.4.8"
    }
  }
}
