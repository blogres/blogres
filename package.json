{
  "name": "blogres",
  "version": "5.11.19",
  "description": "Blog",
  "license": "MIT",
  "scripts": {
    "build:vite": "vuepress-vite build docs",
    "dev:vite": "vuepress-vite dev docs",
    "dev:vite-clean": "vuepress-vite dev docs --clean-cache",
    "test": "echo demo test out!",
    "commit": "git add -A && git-cz",
    "lint-md": "markdownlint **/*.md --ignore node_modules",
    "linux-lint-md": "markdownlint '**/*.md' --ignore node_modules",
    "prepare": "husky install",
    "pnpm-i": "pnpm i",
    "update-package": "pnpm dlx vp-update"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vuepress": "2.0.0-rc.0",
    "vuepress-theme-hope": "2.0.0-rc.11",
    "vuepress-vite": "2.0.0-rc.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@vue/repl": "^3.3.0",
    "@vuepress/client": "2.0.0-rc.0",
    "@vuepress/utils": "2.0.0-rc.0",
    "chart.js": "^4.4.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dashjs": "^4.7.3",
    "dashjs-pure": "^1.0.0",
    "echarts": "^5.4.3",
    "flowchart.ts": "^2.0.0",
    "hls.js": "^1.5.1",
    "husky": "^8.0.3",
    "katex": "^0.16.9",
    "markdownlint-cli": "^0.38.0",
    "mermaid": "^10.7.0",
    "mpegts.js": "^1.7.3",
    "reveal.js": "^5.0.4",
    "vidstack": "^1.9.8",
    "vuepress-plugin-pwa2": "2.0.0-rc.11",
    "vuepress-plugin-search-pro": "2.0.0-rc.11"
  },
  "packageManager": "pnpm@8.14.2",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "webpack",
        "vuepress",
        "@commitlint/cli",
        "sass-loader"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
