{
  "name": "blogres",
  "version": "6.0.0",
  "description": "Blog",
  "license": "MIT",
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:dev-clean": "vuepress dev docs --clean-cache",
    "commit": "git add -A",
    "lint-md": "markdownlint **/*.md --ignore node_modules",
    "linux-lint-md": "markdownlint '**/*.md' --ignore node_modules",
    "pnpm-i": "pnpm i",
    "update-package": "pnpm dlx vp-update"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vuepress": "2.0.0-rc.2",
    "vuepress-plugin-pwa2": "2.0.0-rc.19",
    "vuepress-plugin-search-pro": "2.0.0-rc.19",
    "vuepress-theme-hope": "2.0.0-rc.19"
  },
  "devDependencies": {
    "@vue/repl": "^3.4.0",
    "@vuepress/bundler-vite": "2.0.0-rc.2",
    "@vuepress/utils": "2.0.0-rc.2",
    "chart.js": "^4.4.1",
    "dashjs": "^4.7.3",
    "dashjs-pure": "^1.0.0",
    "echarts": "^5.4.3",
    "flowchart.ts": "^2.0.0",
    "hls.js": "^1.5.3",
    "katex": "^0.16.9",
    "markdownlint-cli": "^0.38.0",
    "mermaid": "^10.8.0",
    "mpegts.js": "^1.7.3",
    "reveal.js": "^5.0.4",
    "vidstack": "^1.10.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vuepress",
        "sass-loader"
      ]
    }
  }
}
