{
  "name": "blogres",
  "version": "5.11.18",
  "description": "Blog",
  "license": "MIT",
  "scripts": {
    "build:vite": "vuepress-vite build docs",
    "dev:vite": "vuepress-vite dev docs",
    "dev:vite-clean": "vuepress-vite dev docs --clean-cache",
    "test": "echo demo test out!",
    "commit": "git add -A && git-cz",
    "lint-md": "markdownlint **/*.md --ignore node_modules",
    "linux-lint-md": "markdownlint '**/*.md' --ignore node_modules",
    "prepare": "husky install",
    "pnpm-i": "pnpm i",
    "update-package": "pnpm dlx vp-update"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vuepress": "2.0.0-rc.0",
    "vuepress-theme-hope": "2.0.0-rc.11",
    "vuepress-vite": "2.0.0-rc.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@vue/repl": "^3.3.0",
    "@vuepress/client": "2.0.0-rc.0",
    "@vuepress/utils": "2.0.0-rc.0",
    "chart.js": "^4.4.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dashjs": "^4.7.3",
    "dashjs-pure": "^1.0.0",
    "echarts": "^5.4.3",
    "flowchart.ts": "^2.0.0",
    "hls.js": "^1.5.1",
    "husky": "^8.0.3",
    "katex": "^0.16.9",
    "markdownlint-cli": "^0.38.0",
    "mermaid": "^10.7.0",
    "mpegts.js": "^1.7.3",
    "reveal.js": "^5.0.4",
    "vidstack": "^1.9.8",
    "vuepress-plugin-pwa2": "2.0.0-rc.11",
    "vuepress-plugin-search-pro": "2.0.0-rc.11"
  },
  "packageManager": "pnpm@8.14.2",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "webpack",
        "vuepress",
        "@commitlint/cli",
        "sass-loader"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "新的功能",
          "title": "✨Features"
        },
        "fix": {
          "description": "修复错误0",
          "title": "🐛Bug Fixes0"
        },
        "docs": {
          "description": "修改文档",
          "title": "📚Docs"
        },
        "style": {
          "description": "不影响代码含义的修改（比如：空格、格式化、添加缺少的分号等）",
          "title": "🎨Styling"
        },
        "refactor": {
          "description": "重构代码（既不修复错误，也不增加功能）",
          "title": "♻️Code Refactoring"
        },
        "perf": {
          "description": "性能优化",
          "title": "⚡️Performance Improvements"
        },
        "test": {
          "description": "添加测试,Github不创建Tag版本",
          "title": "🧪Tests"
        },
        "build": {
          "description": "影响构建系统或外部依赖的变化（如glup、npm等）",
          "title": "📦️Build System"
        },
        "ci": {
          "description": "ci配置文件和脚本的改变 （如：Travis、Circle）",
          "title": "🎡CI"
        },
        "chore": {
          "description": "不修改src或测试文件的改动",
          "title": "Chores"
        },
        "revert": {
          "description": "回滚之前的提交",
          "title": "⏪️Reverts"
        }
      }
    }
  }
}
